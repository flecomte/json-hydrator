{
    "name": "fle/json-hydrator",
    "description": "This library is an alternative to an ORM like doctrine",
    "keywords": ["postgresql", "orm", "json", "hydrator", "serializer"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabrice Lecomte",
            "email": "rusk23@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-ctype": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-pdo_pgsql": "*",
        "jms/serializer": "^2.0.0",
        "ramsey/uuid": "^3.8",
        "symfony/console": "4.2.*",
        "symfony/form": "4.2.*",
        "pagerfanta/pagerfanta": "2.*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14",
        "phpunit/phpunit": "7.5.*",
        "symfony/browser-kit": "4.2.*",
        "symfony/css-selector": "4.2.*",
        "symfony/debug-bundle": "4.2.*",
        "symfony/stopwatch": "4.2.*",
        "symfony/var-dumper": "4.2.*"
    },
    "autoload": {
        "psr-4": {
            "FLE\\JsonHydrator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FLE\\JsonHydrator\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability" : "dev",
    "prefer-stable": true
}

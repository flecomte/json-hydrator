{
    "name": "fle/json-hydrator",
    "description": "This library is an alternative to an ORM like doctrine",
    "keywords": ["postgresql", "orm", "json", "hydrator", "serializer"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabrice Lecomte",
            "email": "fabrice.lecomte.be@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3",
        "ext-ctype": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-pdo_pgsql": "*",
        "jms/serializer": "^3",
        "ramsey/uuid": "^4",
        "symfony/console": "^5",
        "symfony/form": "^5",
        "pagerfanta/pagerfanta": "^2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2",
        "phpunit/phpunit": "7.5.*",
        "symfony/browser-kit": "^5",
        "symfony/css-selector": "^5",
        "symfony/debug-bundle": "^5",
        "symfony/stopwatch": "^5",
        "symfony/var-dumper": "^5"
    },
    "autoload": {
        "psr-4": {
            "FLE\\JsonHydrator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FLE\\JsonHydrator\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "minimum-stability" : "dev",
    "prefer-stable": true
}
